State} from 'react'\nimport { Input } from '../Input/Input'\nimport { useDispatch } from 'react-redux'\nimport { useHistory } from 'react-router-dom'\nimport { signin, signup } from '../../actions/auth'\n\nconst initialState = {email: '', password: ''}\n\nexport const Auth = ({}) => {\n    \n    const [isSignup, setIsSignup] = useState(false)\n    const [formData, setFormData] = useState(initialState)\n    const dispatch = useDispatch()\n    const history = useHistory();\n    const handleSubmit = (e) => {\n        e.preventDefault();\n        if (isSignup){\n            dispatch(signup(formData, history))\n        } else {\n            dispatch(signin(formData, history))\n        }\n    }\n\n    const handleChange = (e) => {\n        setFormData({...formData, [e.target.name]: e.target.value})\n    }\n\n    return (\n        <div>\n            <form onSubmit={handleSubmit}>\n                <Input handleChange={handleChange} name='email' type='text' />\n                <Input handleChange={handleChange} name='password' type='text' />\n                <button type=\"submit\">{isSignup ? 'Sign up' : 'Sign in'}</button>\n                <button type=\"button\" onClick={()=> setIsSignup(!isSignup)}>Switch</button>\n            </form>\n        </div>\n    )\n}\n","/home/alexey/projects/resume_with_blog/client/src/views/Education/Education.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Contacts/Contacts.js",["225","226","227","228","229","230"],"import React from 'react'\nimport './contacts.scss'\nimport { Icon, BrandIcon } from '../common/Icon/Icon';\n\nexport const Contacts = ({list}) => {\n    return (\n        <ul className=\"contacts\">\n            { list.map((item, index) => <Contact key={index} {...item}/>) }\n            <li className=\"contacts__item\">\n                {<div className=\"contacts__icon\"><Icon className='contacts__svg' size={30} name='social'/></div>}\n                <div className=\"contacts__data\">\n                    <div 