tePost={deletePost}\n                likePost={likePost}\n                dispatch={dispatch}\n                >\n                    {post.message}\n                </Post>) \n            }\n        </div>\n    )\n}\n\nexport const Post = ({ post, setCurrentId, dispatch, deletePost, likePost }) => {\n\n    const { _id, creator, title, message, tags, selectedFile, createdAt, likes  } = post\n    const user = useSelector((state) => state.auth)\n    const isOwner = post.creator === user?.authData?.result._id;\n    return (\n        <div className=\"post posts__item\">\n            <div className=\"post__date_container\">\n                <img className=\"post__image\" src={`http://localhost:5000/uploads/${selectedFile}`}  alt=\"post_img\"/>\n                <div className=\"post__date\">{moment(createdAt).fromNow()}</div>\n            </div>\n            <div className=\"post__title\">{title}</div>\n            <div className=\"post__message\">{message}</div>\n            <div className=\"post__meta\">{tags.map((tag) => `#${tag}`)}</div>\n            {isOwner &&<button onClick={() => {setCurrentId(_id)}}>Update</button>}\n            {isOwner && <button onClick={() => {dispatch(deletePost(post._id))}}>Delete</button> }\n            <button onClick={() => {dispatch(likePost(post._id))}}>Like {likes.length}</button>\n        </div>\n    )\n}\n","/home/alexey/projects/resume_with_blog/client/src/actions/postCategory.js",[],"/home/alexey/projects/resume_with_blog/client/src/constants/common.js",[],"/home/alexey/projects/resume_with_blog/client/src/constants/actionTypes.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Header/Header.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Content/Content.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Footer/Footer.js",[],"/home/alexey/projects/resume_with_blog/client/src/actions/auth.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Input/Input.js",[],"/home/alexey/projec