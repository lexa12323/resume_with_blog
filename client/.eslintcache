/src/constants/actionTypes.js",[],"/home/alexey/projects/resume_with_blog/client/src/actions/auth.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Header/Header.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Content/Content.js",[],"/home/alexey/projects/resume_with_blog/client/src/api/setHeaders.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Footer/Footer.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Input/Input.js",[],"/home/alexey/projects/resume_with_blog/client/src/mock/menu.js",[],"/home/alexey/projects/resume_with_blog/client/src/service/user.service.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Logo/Logo.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Navbar/Navbar.js",[],"/home/alexey/projects/resume_with_blog/client/src/components/Page404.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Posts/Posts.tsx",["229","230","231"],"import React from 'react'\nimport moment from 'moment'\nimport PropTypes from 'prop-types';\nimport { useSelector, useDispatch } from 'react-redux'\nimport { Icon } from '../common/Icon/Icon';\nimport { IUser } from '../Auth/interface'\nimport './posts.scss'\n\ntype PostsProps = {\n    posts: any[],\n    setCurrentId: (id: string ) => any,\n    deletePost: (id: string ) => any,\n    likePost: (id: string ) => any,\n    filter: string\n}\n\nexport const Posts: React.FC<PostsProps> = ({posts, setCurrentId, deletePost, likePost, filter}) => {\n    const filteredPosts = filter ? posts.filter(post => post.category && post.category._id === filter) : posts;\n    return (\n        <div className=\"posts\">\n            { filteredPosts.map((post) => <Post \n                key={post._id} \n                post={post} \n                setCurrentId={setCurrentId}\n                deletePost={deletePost}\n                likePost={likePost}\n                >\n                    {post.message}\n                </Post>) \n            }\n        </div>\n    )\n}\n\ninterface PostInterface {\n    _id: string,\n    creator: {\n        _id:string\n    },\n    category: {\n        name: string\n    },\n    title: string,\n    message: string,\n    tags: string[],\n    selectedFile: string,\n    createdAt:string,\n    likes: string[]\n}\n\ntype PostProps = {\n    post: PostInterface,\n    setCurrentId: (id: string ) => void,\n    deletePost: (id: string ) => void,\n    likePost: (id: string ) => void,\n}\n\nexport const Post: React.FC<PostProps> = ({ post, setCurrentId, deletePost, likePost }) => {\n\n    const { _id, creator, category, title, message, tags, selectedFile, createdAt, likes  } = post\n    const user = useSelector((state: IUser) => state.auth)\n    const isOwner = post?.creator?._id === user?.authData?.result._id;\n    return (\n        <div className=\"post posts__item\">\n            <div className=\"post__date_container\">\n                <img className=\"post__image\" src={`/sources/uploads/${selectedFile}`}  alt=\"post_img\"/>\n                <div className=\"post__date\">{moment(createdAt).fromNow()}</div>\n            </div>\n            <div className=\"post__title\">{title}</div>\n            <div className=\"post__message\">{message}</div>\n            { category && <div className=\"post__category\">Категория: {category?.name}</div> }\n            { /*creator && <div className=\"post__creator\">Creator: {creator?.email}</div>*/ }\n            <div className=\"post__meta\">{tags && tags.map((tag) => `#${tag} `)}</div>\n            {isOwner && <button onClick={() => {setCurrentId(_id)}}>Update</button>}\n            {isOwner && <button onClick={() => {deletePost(post._id)}}>Delete</button> } \n            \n            <button className=\"post__like\" onClick={() => {likePost(post._id)}}>\n                <div>\n                    <Icon size=\"16\" fill=\"#f4bf00\" name='like'/>\n                </div> \n                {likes && likes.length}\n            </button>\n        </div>\n    )\n}\n\n/*Post.propTypes = {\n    post: PropTypes.object,\n    setCurrentId: PropTypes.func,\n    deletePost: PropTypes.func,\n    likePost: PropTypes.func,\n};/*\n\nPost.defaultProps = {\n    post: {},\n    setCurrentId: () => {},\n    deletePost: () => {},\n    likePost: () => {},\n};*/\n",["232","233"],"/home/alexey/projects/resume_with_blog/client/src/views/common/Icon/Icon.tsx",["234","235"],"import React, {useState, useEffect} from 'react';\nimport PropTypes from 'prop-types';\nimport { useIconLoader } from './useIconLoader'\n\n//TODO brands and selection is too large. Bandle too large\n\ninterface IIconElement{\n  tags: string[]\n}\n\ninterface IIconObject{\n  attrs: []\n}\n\nexport interface IIconProps {\n  fill?: string,\n  name: string,\n  styles?: string[],\n  size?: string,\n}\n\nexport interface IMoonIconProps extends IIconProps{\n  iconSet?: {\n    icons: any[],\n    selection?: any[],\n    metadata: {},\n    height: number,\n    prevSize: number,\n    colorThemes?: string[],\n    preferences: {},\n    IcoMoonType: string\n  },\n}\n\nexport const Icon: React.FC<IIconProps> = (props) => {\n    //const decoratorProps = {...props, iconSet: iconSet}\n    const {iconsData , iconsLoaded} = useIconLoader(props, 'selection')\n    if (!iconsLoaded) return null\n    return (\n        < MoonIcon {...iconsData }/>\n    )\n};\n\nexport const BrandIcon: React.FC<IIconProps> = (props) => {\n  const {iconsData , iconsLoaded} = useIconLoader(props, 'brands')\n  if (!iconsLoaded) return null\n    return (\n        < MoonIcon {...iconsData }/>\n    )\n};\n\nconst MoonIcon: React.FC<IMoonIconProps> = ({iconSet, name, styles, size = '24', ...rest}) => {\n    const find = (iconEl: IIconElement) => iconEl.tags.includes(name);\n    const currentIcon = iconSet && iconSet.icons.find(find);\n    const renderPath = (iconObj: IIconObject) => (path: string, index: number) => {\n      const attrs = (iconObj!.attrs && iconObj!.attrs[index]) || {};\n      return <path /*style={styles.path}*/ key={index} d={path} {...attrs} />;\n    };\n  \n    if (currentIcon) {\n      return (\n        <svg\n          /*style={styles.svg}*/\n          width={size}\n          height={size}\n          viewBox={`0 0 1024 1024`}\n          xmlns=\"http://www.w3.org/2000/svg\"\n          {...rest}\n        >\n          {currentIcon.paths.map(renderPath(currentIcon))}\n        </svg>\n      );\n    }\n    return null;\n}\n\nIcon.propTypes = {\n    name: PropTypes.string.isRequired,\n};","/home/alexey/projects/resume_with_blog/client/src/views/common/Icon/useIconLoader.tsx",["236"],"import { useState, useEffect } from 'react'\nimport { IMoonIconProps, IIconProps } from './Icon'\n\nexport const useIconLoader = (props:IIconProps, iconJson: string) => {\n    \n    const [iconsData, setIconsData] = useState<IMoonIconProps>({...props})\n    const [iconsLoaded, setIconsLoaded] = useState(false)\n    useEffect(()=> {\n      \n      const getIcons = async () => {\n        const icons = await import(`./${iconJson}.json`) \n        setIconsLoaded(true)\n        setIconsData((prevState) => ({...prevState, iconSet: icons }))\n      }\n       getIcons()\n       console.log('get icons ' + iconJson)\n      \n    }, [])\n\n    return { iconsData, iconsLoaded }\n}","/home/alexey/projects/resume_with_blog/client/src/reducers/postCategories.tsx",[],{"ruleId":"237","replacedBy":"238"},{"ruleId":"239","replacedBy":"240"},{"ruleId":"241","severity":1,"message":"242","line":1,"column":16,"nodeType":"243","messageId":"244","endLine":1,"endColumn":25},{"ruleId":"241","severity":1,"message":"245","line":1,"column":27,"nodeType":"243","messageId":"244","endLine":1,"endColumn":35},{"ruleId":"241","severity":1,"message":"246","line":7,"column":10,"nodeType":"243","messageId":"244","endLine":7,"endColumn":22},{"ruleId":"247","severity":1,"message":"248","line":29,"column":8,"nodeType":"249","endLine":29,"endColumn":29,"suggestions":"250"},{"ruleId":"251","severity":1,"message":"252","li