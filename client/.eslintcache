ories)\n\n    useEffect(() => {\n        dispatch(getPosts())\n        !categories.loaded && !categories.loading && dispatch(getCategories())\n    }, [currentId, dispatch])\n\n    return(\n        <Layout>\n            <Page title={title} subtitle={subtitle} description={description} darken>\n                <Posts \n                    setCurrentId={setCurrentId} \n                    posts={posts} \n                    deletePost={deletePost} \n                    likePost={likePost} \n                    dispatch={dispatch}\n                />\n                {user.authData && categories.loaded && <Form currentId={currentId} categories={categories.list} setCurrentId={setCurrentId}/> }\n            </Page>\n        </Layout>\n    )\n}\n\nexport default PostsContainer","/home/alexey/projects/resume_with_blog/client/src/reducers/posts.js",[],"/home/alexey/projects/resume_with_blog/client/src/reducers/auth.js",[],"/home/alexey/projects/resume_with_blog/client/src/reducers/postCategories.js",[],"/home/alexey/projects/resume_with_blog/client/src/mock/education.js",[],"/home/alexey/projects/resume_with_blog/client/src/mock/contacts.js",[],"/home/alexey/projects/resume_with_blog/client/src/mock/exprience.js",[],"/home/alexey/projects/resume_with_blog/client/src/helpers/yearsDiff.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Signature/Signature.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Page/Page.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Education/Education.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Layout/Layout.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Exprience/Exprience.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Achievements/Achievements.js",[],"/home/alexey/projects/resume_with_blog/client/src/views/Contacts/Contacts.js",[],"/home/alexey/projects/resume_with_blog/client/src/actions/posts.js",[],"/home/alexey/projects/resume_with_blog/client/sr